{
  "gitbook": ">=3.2.1",
  "root": "_docs",
  "title": "Opentracing",
  "plugins": ["fontsettings", "edit-link", "github", "-search", "scripts"],
  "pluginsConfig": {
     "scripts": {
        "files": [
          "scripts/injectLogo.js"
        ]
    },
    "fontSettings": {
      "theme": "white",
       "size": 3
    },
    "edit-link": {
      "base": "https://github.com/opentracing/opentracing/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/opentracing/opentracing.io/"
    },
    "theme-default": {
      "showLevel": true
    }
  }
}