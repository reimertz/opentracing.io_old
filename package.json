  "main": "index.js",
  "scripts": {
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve ./ documentation",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build ./ documentation",
    "css": "node-sass _sass/main.scss -o css",
    "css:watch": "npm run css && node-sass _sass/main.scss -wo css",
    "dev": "jekyll serve --config _config.yml & npm run css:watch"
  },
